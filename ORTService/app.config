<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <section name="LogFlags" type="System.Configuration.NameValueSectionHandler" />
        <section name="TcpFlags" type="System.Configuration.NameValueSectionHandler" />
    </configSections>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
    </startup>
    <LogFlags>
        <add key="LogDebug" value="1" />
        <add key="LogSession" value="1" />
        <add key="DebugFilename" value="C:\Tmp\ort_debug.txt" />
        <add key="SessionFilename" value="C:\Tmp\ort_session.txt" />
    </LogFlags>
    <TcpFlags>
        <add key="ORTDeviceServerPort" value="3333" />
        <add key="ORTCommandServerPort" value="8888" />
    </TcpFlags>
</configuration>
